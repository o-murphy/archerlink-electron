{
  "name": "rtsp-player",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "express": "^4.19.2",
    "fluent-ffmpeg": "^2.1.2",
    "sharp": "^0.33.4",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5"
  },
  "devDependencies": {
    "electron": "^31.0.2",
    "electron-builder": "^24.13.3"
  },
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder",
    "build:win32": "electron-builder build --win --ia32"
  },
  "main": "electron.mjs",
  "build": {
    "appId": "com.example.rtsp-player",
    "files": [
      "./*.mjs"
    ],
    "extraFiles": [
      "./ffmpeg.exe",
      "./pwa/**/*"
    ],
    "extraResources": [],
    "extraMetadata": {
      "main": "electron.mjs"
    },
    "win": {
      "target": "nsis",
      "icon": "pwa/favicon.ico"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
