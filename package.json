{
  "name": "rtsp-player",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "socket.io-client": "^4.7.5",
    "socket.io": "^4.7.5",
    "fluent-ffmpeg": "^2.1.2"
  },
  "devDependencies": {
    "electron": "^13.0.0",
    "electron-builder": "^22.10.5"
  },
  "scripts": {
    "start": "react-scripts --openssl-legacy-provider start",
    "build": "react-scripts --openssl-legacy-provider build",
    "test": "react-scripts --openssl-legacy-provider test",
    "eject": "react-scripts eject",
    "electron": "set NODE_OPTIONS=--openssl-legacy-provider electron .",
    "dist": "set NODE_OPTIONS=--openssl-legacy-provider electron-builder",
    "main": "set NODE_OPTIONS=--openssl-legacy-provider electron.mjs"
  },
  "build": {
    "appId": "com.example.rtsp-player",
    "win": {
      "target": "nsis",
      "icon": "path/to/icon.ico"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
